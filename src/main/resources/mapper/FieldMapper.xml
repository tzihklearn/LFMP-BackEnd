<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.MinNiCup.lfmpbackend.mapper.FieldMapper">

    <select id="selectIdByFieldFirst" resultType="java.lang.Integer">
        SELECT id
        FROM field
        WHERE field_first = #{fieldFirst,jdbcType=VARCHAR}
    </select>
</mapper>